<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.mapper.LikeCollectMapper">

    <select id="generalQueryCollect" resultType="com.web.domain.Resp.LikeCollectResp">
        SELECT
            lc.*,
            t.${picturesField} as pictures ,
            t.${titleField} as title
        FROM
            like_collect lc
                LEFT JOIN ${tableName} t ON lc.moment_id = t.id
        WHERE
            lc.id IN
            <foreach collection="ids" item="id" open="(" separator="," close=")">
                #{id}
            </foreach>
    </select>
</mapper>